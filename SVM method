641.8680996708114,403.67960398228024,b
662.9265622081788,418.52748578700414,b
648.597047100465,442.7769113845835,b
"""

# Save the data to a CSV file
with open("dataset.csv", "w") as file:
    file.write(soft_data)

# Load the dataset from the CSV file
data_soft_reading = pd.read_csv("dataset.csv")
print(data_soft_reading.shape)

##one hot encoding
my_data_soft=pd.get_dummies(data_soft_reading,columns=['label'],dtype=float)
##spliting features and targets
Xsoft=my_data_soft[['x','y']].to_numpy()
ysoft=my_data_soft[['label_a']].to_numpy()
##definig classifier
clf_soft= SVC(kernel='linear' ,degree=1, C=1)

#defining parameters with cv=3
from sklearn.model_selection import cross_val_score, KFold, GridSearchCV
params = {
    "kernel": [ 'linear','rbf','poly'],
    "C":[1,10,20,100],
    "degree" : [1,2,3,5]}
gs=GridSearchCV(estimator=clf_soft, param_grid=params, cv=3)
# now fit the dataset to the GridSearchCV object.
gs = gs.fit(Xsoft, ysoft.ravel())
y_pred_soft=gs.predict(Xsoft)

import matplotlib.pyplot as plt
import numpy as np

# First, plot the data points for each class
plt.figure(figsize=(8, 6))  # Setting the figure size for better visibility
scatter = plt.scatter(Xsoft[:558, 0], Xsoft[:558, 1],c=ysoft, edgecolor='k', s=90)

# Fetch the current plot's axes for later use in plotting contours
ax = plt.gca()

# Generate a grid of values within the range of the feature space
xx, yy = np.meshgrid(np.linspace(*ax.get_xlim(), 30), np.linspace(*ax.get_ylim(), 30))

# Flatten the grid to pass to the decision function
grid_points = np.c_[xx.ravel(), yy.ravel()]

# Evaluate the decision function for each point on the grid
decision_values = gs.decision_function(grid_points).reshape(xx.shape)

# Plot the decision boundary and margins using contour lines
contours = ax.contour(xx, yy, decision_values, levels=[-1, 0, 1], colors='k', linestyles=['--', '-', '--'], alpha=0.5)
plt.clabel(contours, inline=True, fontsize=10, fmt={-1: 'Margin', 0: 'Decision boundary', 1: 'Margin'})

# Add a title to the plot
plt.title('Soft Margin SVM Decision Boundary')
plt.show()
