560.8621662113112,69.72024676366004,b
563.5586644516357,83.03232951251414,b
520.0406079195876,83.60942411647846,b

"""

# Save the data to a CSV file
with open("dataset.csv", "w") as file:
    file.write(csv_data)

# Load the dataset from the CSV file
data = pd.read_csv("dataset.csv")

from sklearn.preprocessing import LabelEncoder

# Initialize label encoder
le = LabelEncoder()

# Fit and transform labels to numbers using fit_transform()
data['label'] = le.fit_transform(data['label'])

# Prepare feature matrix X and target vector y
X = data[['x', 'y']].values
y = data['label'].values

"""# Hard Margin

"""

from sklearn.svm import SVC
from sklearn.metrics import accuracy_score
clf= SVC(kernel='linear' ,degree=1, C=1e6)
clf.fit(X,y)
y_pred=clf.predict(X)

import matplotlib.pyplot as plt
import numpy as np

# First, plot the data points for each class
plt.figure(figsize=(8, 6))  # Setting the figure size for better visibility
scatter = plt.scatter(X[:, 0], X[:, 1], c=y, edgecolor='k', s=50)

# Fetch the current plot's axes for later use in plotting contours
ax = plt.gca()

# Generate a grid of values within the range of the feature space
xx, yy = np.meshgrid(np.linspace(*ax.get_xlim(), 30), np.linspace(*ax.get_ylim(), 30))

# Flatten the grid to pass to the decision function
grid_points = np.c_[xx.ravel(), yy.ravel()]

# Evaluate the decision function for each point on the grid
decision_values = clf.decision_function(grid_points).reshape(xx.shape)

# Plot the decision boundary and margins using contour lines
contours = ax.contour(xx, yy, decision_values, levels=[-1, 0, 1], colors='k', linestyles=['--', '-', '--'], alpha=0.5)
plt.clabel(contours, inline=True, fontsize=10, fmt={-1: 'Margin', 0: 'Decision boundary', 1: 'Margin'})

# Add a title to the plot
plt.title('Hard Margin SVM Decision Boundary')
plt.show()

"""# Soft Margin for another dataset

"""

# Open an interactive plot for drawing points
draw_scatter()

